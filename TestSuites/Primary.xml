<?xml version="1.0" encoding="UTF-8"?>
<suite name="MyTestSuite" parallel="tests" thread-count="2">
	<listeners>
		<listener
			class-name="rahulshettyacademy.TestComponents.Listeners" />
	</listeners>

	<groups>
		<run>
			<include name="primary" />
		</run>
	</groups>

	<test name="Submit Order Test">
		<classes>
			<class name="rahulshettyacademy.Tests.submitOrder" />
		</classes>
	</test>
	<test name="Error Validations Test">
		<classes>
			<class name="rahulshettyacademy.Tests.ErrorValidations" />
		</classes>
	</test>
</suite>
